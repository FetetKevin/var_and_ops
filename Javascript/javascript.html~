<button onclick='alert(shouldComputeDouble(4))'>Exo 5</button>
<script>
var shouldComputeDouble = function(number){
     var a = 12;


    var multiply = function(b){
    	var a = b*2;
    	return a;
    }
    
    return multiply(number);
};
</script>


