<button onclick='alert(computeSum(2,"5",4))'>Exo 4</button>
<script>
var computeSum = function() {

	var result = 0;

	for (i = 0; i < arguments.length; i++) {
		result += parseInt(arguments[i]);
	}
	return result;

};
</script>


